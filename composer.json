{
    "name": "sulimanbenhalim/laravel-superjson",
    "version": "0.9.0-beta",
    "description": "SuperJSON format for Laravel - preserve JavaScript types in JSON (Beta: CSRF limitations)",
    "type": "library",
    "keywords": [
        "laravel",
        "superjson",
        "json",
        "serialization",
        "types"
    ],
    "authors": [
        {
            "name": "Suliman Benhalim",
            "email": "sulimanbenhalim@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^9.0|^10.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "SulimanBenhalim\\LaravelSuperJson\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SulimanBenhalim\\LaravelSuperJson\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SulimanBenhalim\\LaravelSuperJson\\SuperJsonServiceProvider"
            ],
            "aliases": {
                "SuperJson": "SulimanBenhalim\\LaravelSuperJson\\Facades\\SuperJson"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}